{
    "id": "actors",
    "endpoint": {
        "url": "http://localhost:3030/intavia/query",
        "useAuth": false,
        "prefixesFile": "SparqlQueriesPrefixes.js"
    },
    "sparqlQueriesFile": "SparqlQueriesActors.js",
    "baseURI": "http://www.wikidata.org",
    "URITemplate": "<BASE_URI>/entity/<LOCAL_ID>",
    "facetClass": "wlink:Person",
    "frontPageImage": "main_page/works-452x262.jpg",
    "searchMode": "faceted-search",
    "defaultActiveFacets": [
        "prefLabel"
    ],
    "defaultTab": "table",
    "defaultInstancePageTab": "table",
    "maps": {
        "placesActors": {
            "center": [
                22.43,
                10.37
            ],
            "zoom": 2
        },
        "peopleMigrations": {
            "center": [
                22.43,
                10.37
            ],
            "zoom": 2
        },
        "peopleHeatmap": {
            "center": [
                22.43,
                10.37
            ],
            "zoom": 2
        }
    },
    "resultClasses": {
        "actors": {
            "paginatedResultsConfig": {
                "tabID": 0,
                "component": "ResultTable",
                "tabPath": "table",
                "tabIcon": "CalendarViewDay",
                "propertiesQueryBlock": "actorPropertiesFacetResults",
                "pagesize": 10,
                "sortBy": null,
                "sortDirection": null
            },
            "instanceConfig": {
                "propertiesQueryBlock": "actorProperties",
                "instancePageResultClasses": {
                    "instancePageTable": {
                        "tabID": 0,
                        "component": "InstancePageTable",
                        "tabPath": "table",
                        "tabIcon": "CalendarViewDay"
                    },
                    "instancePageExport": {
                        "tabID": 1,
                        "component": "Export",
                        "tabPath": "export",
                        "tabIcon": "CloudDownload",
                        "pageType": "instancePage"
                    }
                }
            }
        },
        "placesActors": {
            "tabID": 1,
            "component": "LeafletMap",
            "tabPath": "map",
            "tabIcon": "AddLocation",
            "sparqlQuery": "peoplePlacesQuery",
            "facetClass": "actors",
            "filterTarget": "person",
            "resultMapper": "mapPlaces",
            "instanceConfig": {
                "propertiesQueryBlock": "placePropertiesInfoWindow",
                "relatedInstances": "peopleRelatedTo",
                "createPopUpContent": "createPopUpContentLetterSampo"
            }
        },
        "peopleHeatmap": {
            "tabID": 2,
            "component": "Deck",
            "tabPath": "heatmap",
            "tabIcon": "LocationOn",
            "sparqlQuery": "peoplePlacesQuery",
            "facetClass": "actors",
            "filterTarget": "person",
            "resultMapper": "mapPlaces",
            "layerType": "heatmapLayer",
            "heatmapRadiusPixels": 40,
            "heatmapThreshold": 0.025,
            "heatmapIntensity": 1
        },
        "peopleMigrations": {
            "tabID": 3,
            "component": "Deck",
            "tabPath": "migrations",
            "tabIcon": "Redo",
            "sparqlQuery": "peopleMigrationsQuery",
            "facetClass": "actors",
            "filterTarget": "person",
            "layerType": "arcLayer",
            "arcWidthVariable": "instanceCountScaled",
            "instanceVariable": "person",
            "showTooltips": true,
            "postprocess": {
                "func": "linearScale",
                "config": {
                    "variable": "instanceCount",
                    "minAllowed": 3,
                    "maxAllowed": 30
                }
            }
        },
        "peopleMigrationsDialog": {
            "sparqlQuery": "peopleMigrationsDialogQuery",
            "filterTarget": "id"
        },
        "actorNetwork": {
            "tabID": 4,
            "component": "Network",
            "tabPath": "network",
            "tabIcon": "BubbleChart",
            "sparqlQuery": "networkLinksQuery",
            "facetClass": "actors",
            "sparqlQueryNodes": "networkNodesFacetQuery",
            "filterTarget": "actor",
            "useNetworkAPI": true,
            "limit": 200,
            "optimize": 1.5,
            "preprocess": "preprocessLetterSampo",
            "style": [
                {
                    "selector": "node",
                    "style": {
                        "shape": "ellipse",
                        "font-size": 10,
                        "background-color": "data(color)",
                        "label": "data(prefLabel)",
                        "height": "data(size)",
                        "width": "data(size)"
                    }
                },
                {
                    "selector": "edge",
                    "style": {
                        "width": "data(weight)",
                        "line-color": "data(color)",
                        "curve-style": "bezier",
                        "content": "data(prefLabel)",
                        "color": "hsl(30, 64%, 35%)",
                        "target-arrow-shape": "triangle",
                        "target-arrow-color": "data(color)",
                        "font-size": "8",
                        "text-valign": "center",
                        "edge-text-rotation": "autorotate",
                        "text-background-opacity": 1,
                        "text-background-color": "white",
                        "text-background-shape": "roundrectangle"
                    }
                }
            ],
            "layout": {
                "name": "cose",
                "idealEdgeLength": 100,
                "nodeOverlap": 20,
                "refresh": 20,
                "fit": true,
                "padding": 30,
                "randomize": false,
                "componentSpacing": 100,
                "nodeRepulsion": 400000,
                "edgeElasticity": 100,
                "nestingFactor": 5,
                "gravity": 80,
                "numIter": 1347,
                "initialTemp": 200,
                "coolingFactor": 0.95,
                "minTemp": 1
            }
        },
        "export": {
            "tabID": 5,
            "component": "Export",
            "tabPath": "export",
            "tabIcon": "CloudDownload",
            "resultClass": "actors",
            "facetClass": "actors"
        },
        "knowledgeGraphMetadata": {
            "sparqlQuery": "knowledgeGraphMetadataQuery",
            "resultMapper": "makeObjectList"
        }
    },
    "properties": [
